<?xml version="1.0" encoding="UTF-8" standalone="yes"?><userScript xmlns="http://www.approuter.com/schemas/2003/1/UserCallouts/"><functionName>checkDateTimeBefore</functionName><arguments><argument required="true" type="string" name="in1"/><argument required="true" type="string" name="in2"/></arguments><result type="string"/><body>var olddate=new Date(in2);
var currentdate = new Date(in1);
var flag;
if(currentdate &lt;= olddate)
{
flag=1;
}
else
{
flag=0;
}
return flag;</body></userScript>