<?xml version="1.0" encoding="UTF-8" standalone="yes"?><userScript xmlns="http://www.approuter.com/schemas/2003/1/UserCallouts/"><functionName>formatDateForJira</functionName><arguments><argument required="true" type="string" name="in1"/></arguments><result type="string"/><body>var inDate=new Date(in1);
inDate=dateFormat(inDate, "dd/mmm/yy hh:MM TT");
var out1=inDate.toString();
return out1;
